RewriteEngine On
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteRule ^(.*)$ /tkphoto/index.php?/$1 [L]

php_value upload_max_filesize 11M
php_value post_max_size 11M

#RewriteBase /

